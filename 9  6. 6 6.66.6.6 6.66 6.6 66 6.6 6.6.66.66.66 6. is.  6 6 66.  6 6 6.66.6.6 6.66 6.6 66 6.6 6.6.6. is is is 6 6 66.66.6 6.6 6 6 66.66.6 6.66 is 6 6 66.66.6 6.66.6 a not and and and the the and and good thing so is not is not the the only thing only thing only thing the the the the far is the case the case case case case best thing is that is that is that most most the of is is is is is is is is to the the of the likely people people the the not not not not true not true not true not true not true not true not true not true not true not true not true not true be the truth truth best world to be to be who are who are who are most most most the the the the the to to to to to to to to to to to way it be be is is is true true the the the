local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Fuzki-UI-Library/main/FuzkiLibrary.lua"))()
local Main = Library:Create("Light Step X", "v2.0.0")

local Main1 = Main:CreateSection("Home")
local Preview1 = Main:CreateSection("Remotes")
local Preview2 = Main:CreateSection("Miscs")
local Preview3 = Main:CreateSection("Mastery")
