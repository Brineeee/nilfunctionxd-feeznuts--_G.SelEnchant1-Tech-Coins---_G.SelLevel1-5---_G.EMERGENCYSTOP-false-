Main1:CreateLabel("Important")

Main1:CreateButton("Click", "Official Website", function()
setclipboard("https://www.linktr.ee/brine777")
end) 

Main1:CreateButton("Click", "About", function()
   game.StarterGui:SetCore("SendNotification", {
    Title = "Golden Tool";
    Text = "Creator = goldenwars._.official";
    Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
    Duration = 3;
})
wait(3)
   game.StarterGui:SetCore("SendNotification", {
    Title = "Golden Tool";
    Text = "Discord = mech#0945";
    Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
    Duration = 3;
})
wait(3)
   game.StarterGui:SetCore("SendNotification", {
    Title = "Golden Tool";
    Text = "Roblox = kyleroblox225";
    Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
    Duration = 3;
})
end) 

